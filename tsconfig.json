{
  "compilerOptions": {
    "module": "commonjs",
    "target": "es2017",
    "declaration": false,
    "noImplicitAny": false,
    "sourceMap": false,
    "removeComments": true,
    "strictNullChecks": true,
    "outDir": "build/server",
    "lib": [
      "es2017",
      "esnext.asynciterable"
    ],
    "typeRoots": ["server/types", "node_modules/@types"]
  },
  "exclude": [
    "node_modules"
  ],
  "include": [
    "server/**/*"
  ]
}
